TD_analysis_tb.analyze_sequence ;
    %vpi_call 2 30 "$display", "\012--- Analyzing input %b (decimal %d) ---", v0x55599e2c5090_0, v0x55599e313480_0 {0 0 0};
    %load/vec4 v0x55599e313480_0;
    %pad/u 32;
    %addi 3, 0, 32;
    %load/vec4 v0x55599e313480_0;
    %pad/u 32;
    %addi 3, 0, 32;
    %vpi_call 2 31 "$display", "Excess-3 would be: %d (%b)", S<1,vec4,u32>, S<0,vec4,u32> {2 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55599e313620_0, 0, 1;
    %delay 15, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55599e313620_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 35 "$display", "After reset: State=%d, S=%b, V=%b", v0x55599e2c57c0_0, v0x55599e3136c0_0, v0x55599e3137c0_0 {0 0 0};
    %load/vec4 v0x55599e2c5090_0;
    %parti/s 1, 3, 3;
    %store/vec4 v0x55599e313890_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 39 "$display", "After bit[3]=%b: State=%d, S=%b, V=%b", &PV<v0x55599e2c5090_0, 3, 1>, v0x55599e2c57c0_0, v0x55599e3136c0_0, v0x55599e3137c0_0 {0 0 0};
    %load/vec4 v0x55599e2c5090_0;
    %parti/s 1, 2, 3;
    %store/vec4 v0x55599e313890_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 41 "$display", "After bit[2]=%b: State=%d, S=%b, V=%b", &PV<v0x55599e2c5090_0, 2, 1>, v0x55599e2c57c0_0, v0x55599e3136c0_0, v0x55599e3137c0_0 {0 0 0};
    %load/vec4 v0x55599e2c5090_0;
    %parti/s 1, 1, 2;
    %store/vec4 v0x55599e313890_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 43 "$display", "After bit[1]=%b: State=%d, S=%b, V=%b", &PV<v0x55599e2c5090_0, 1, 1>, v0x55599e2c57c0_0, v0x55599e3136c0_0, v0x55599e3137c0_0 {0 0 0};
