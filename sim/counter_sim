#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5610a062dfc0 .scope module, "counter_tb" "counter_tb" 2 4;
 .timescale -9 -12;
v0x5610a0658bd0_0 .var "clk", 0 0;
v0x5610a0658c90_0 .net "count", 3 0, v0x5610a06588b0_0;  1 drivers
v0x5610a0658d60_0 .var "enable", 0 0;
v0x5610a0658e60_0 .var "reset", 0 0;
S_0x5610a063fe80 .scope module, "uut" "counter" 2 15, 3 4 0, S_0x5610a062dfc0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "enable";
    .port_info 3 /OUTPUT 4 "count";
v0x5610a06400f0_0 .net "clk", 0 0, v0x5610a0658bd0_0;  1 drivers
v0x5610a06588b0_0 .var "count", 3 0;
v0x5610a0658990_0 .net "enable", 0 0, v0x5610a0658d60_0;  1 drivers
v0x5610a0658a60_0 .net "reset", 0 0, v0x5610a0658e60_0;  1 drivers
E_0x5610a0606b00 .event posedge, v0x5610a0658a60_0, v0x5610a06400f0_0;
    .scope S_0x5610a063fe80;
T_0 ;
    %wait E_0x5610a0606b00;
    %load/vec4 v0x5610a0658a60_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5610a06588b0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5610a0658990_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5610a06588b0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x5610a06588b0_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5610a062dfc0;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x5610a0658bd0_0;
    %inv;
    %store/vec4 v0x5610a0658bd0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5610a062dfc0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5610a0658bd0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5610a0658e60_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5610a0658d60_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5610a0658e60_0, 0, 1;
    %vpi_call 2 35 "$display", "Counter Testbench Starting..." {0 0 0};
    %vpi_call 2 36 "$display", "Time\011reset\011enable\011count" {0 0 0};
    %vpi_call 2 37 "$display", "----\011-----\011------\011-----" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5610a0658d60_0, 0, 1;
    %pushi/vec4 20, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 10000, 0;
    %vpi_call 2 43 "$display", "%0t\011%b\011%b\011%b", $time, v0x5610a0658e60_0, v0x5610a0658d60_0, v0x5610a0658c90_0 {0 0 0};
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5610a0658d60_0, 0, 1;
    %pushi/vec4 5, 0, 32;
T_2.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.3, 5;
    %jmp/1 T_2.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 10000, 0;
    %vpi_call 2 50 "$display", "%0t\011%b\011%b\011%b (disabled)", $time, v0x5610a0658e60_0, v0x5610a0658d60_0, v0x5610a0658c90_0 {0 0 0};
    %jmp T_2.2;
T_2.3 ;
    %pop/vec4 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5610a0658d60_0, 0, 1;
    %delay 30000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5610a0658e60_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 58 "$display", "%0t\011%b\011%b\011%b (reset)", $time, v0x5610a0658e60_0, v0x5610a0658d60_0, v0x5610a0658c90_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5610a0658e60_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 62 "$display", "%0t\011%b\011%b\011%b (after reset)", $time, v0x5610a0658e60_0, v0x5610a0658d60_0, v0x5610a0658c90_0 {0 0 0};
    %pushi/vec4 10, 0, 32;
T_2.4 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.5, 5;
    %jmp/1 T_2.5, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 10000, 0;
    %vpi_call 2 67 "$display", "%0t\011%b\011%b\011%b", $time, v0x5610a0658e60_0, v0x5610a0658d60_0, v0x5610a0658c90_0 {0 0 0};
    %jmp T_2.4;
T_2.5 ;
    %pop/vec4 1;
    %vpi_call 2 70 "$display", "\012Counter Testbench Completed." {0 0 0};
    %vpi_call 2 71 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench/counter_tb.v";
    "src/counter.v";
