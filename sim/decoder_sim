    %delay 10000, 0;
    %vpi_call 2 46 "$display", "%0t\011%b\011%b\011%b", $time, v0x55c13b4f1c20_0, v0x55c13b4f1b50_0, v0x55c13b4f1cf0_0 {0 0 0};
