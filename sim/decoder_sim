S_0x55c13b4c7790 .scope module, "decoder_tb" "decoder_tb" 2 4;
 .timescale -9 -12;
v0x55c13b4f1b50_0 .var "enable", 0 0;
v0x55c13b4f1c20_0 .var "in", 1 0;
v0x55c13b4f1cf0_0 .net "out", 3 0, v0x55c13b4f1a10_0;  1 drivers
S_0x55c13b4d8c40 .scope module, "uut" "decoder" 2 14, 3 4 0, S_0x55c13b4c7790;
 .timescale 0 0;
    .port_info 0 /INPUT 2 "in";
    .port_info 1 /INPUT 1 "enable";
    .port_info 2 /OUTPUT 4 "out";
v0x55c13b4d8e90_0 .net "enable", 0 0, v0x55c13b4f1b50_0;  1 drivers
v0x55c13b4f1930_0 .net "in", 1 0, v0x55c13b4f1c20_0;  1 drivers
v0x55c13b4f1a10_0 .var "out", 3 0;
E_0x55c13b49f050 .event anyedge, v0x55c13b4d8e90_0, v0x55c13b4f1930_0;
    .scope S_0x55c13b4d8c40;
T_0 ;
    %wait E_0x55c13b49f050;
    %load/vec4 v0x55c13b4d8e90_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55c13b4f1a10_0, 0, 4;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55c13b4f1930_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55c13b4f1a10_0, 0, 4;
    %jmp T_0.7;
T_0.2 ;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x55c13b4f1a10_0, 0, 4;
    %jmp T_0.7;
T_0.3 ;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x55c13b4f1a10_0, 0, 4;
    %jmp T_0.7;
T_0.4 ;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x55c13b4f1a10_0, 0, 4;
    %jmp T_0.7;
T_0.5 ;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x55c13b4f1a10_0, 0, 4;
    %jmp T_0.7;
T_0.7 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55c13b4c7790;
T_1 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x55c13b4f1c20_0, 0, 2;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c13b4f1b50_0, 0, 1;
    %vpi_call 2 25 "$display", "Decoder Testbench Starting..." {0 0 0};
    %vpi_call 2 26 "$display", "Time\011in\011enable\011out" {0 0 0};
    %vpi_call 2 27 "$display", "----\011--\011------\011----" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c13b4f1b50_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x55c13b4f1c20_0, 0, 2;
    %delay 10000, 0;
    %vpi_call 2 32 "$display", "%0t\011%b\011%b\011%b (disabled)", $time, v0x55c13b4f1c20_0, v0x55c13b4f1b50_0, v0x55c13b4f1cf0_0 {0 0 0};
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x55c13b4f1c20_0, 0, 2;
    %delay 10000, 0;
    %vpi_call 2 35 "$display", "%0t\011%b\011%b\011%b (disabled)", $time, v0x55c13b4f1c20_0, v0x55c13b4f1b50_0, v0x55c13b4f1cf0_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55c13b4f1b50_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x55c13b4f1c20_0, 0, 2;
    %delay 10000, 0;
    %vpi_call 2 40 "$display", "%0t\011%b\011%b\011%b", $time, v0x55c13b4f1c20_0, v0x55c13b4f1b50_0, v0x55c13b4f1cf0_0 {0 0 0};
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x55c13b4f1c20_0, 0, 2;
    %delay 10000, 0;
    %vpi_call 2 43 "$display", "%0t\011%b\011%b\011%b", $time, v0x55c13b4f1c20_0, v0x55c13b4f1b50_0, v0x55c13b4f1cf0_0 {0 0 0};
